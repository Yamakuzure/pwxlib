#pragma once
#ifndef PWX_PWXLIB_SRC_PSINGLERING_INCLUDED
#define PWX_PWXLIB_SRC_PSINGLERING_INCLUDED


/** @file PSingleRing
  * @brief Wraps container/TSingleRing.h and typedefs pwx::TSingleRing to PSingleRing.
**/
#include "container/TSingleRing.h"

/** @typedef PSingleRing
  * @brief Allows to use pwx::TSingleRing outside all namespaces.
**/
template<typename data_t, typename elem_t = ::pwx::TSingleElement<data_t> >
using PSingleRing = ::pwx::TSingleRing<data_t, elem_t>;


#endif // PWX_PWXLIB_SRC_PSINGLERING_INCLUDED
