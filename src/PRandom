#pragma once
#ifndef PWX_PWXLIB_SRC_PRANDOM_INCLUDED
#define PWX_PWXLIB_SRC_PRANDOM_INCLUDED


/** @file PRandom
  * @brief Wraps random/CRandom.h and typedefs pwx::CRandom to PRandom.
**/
#include "random/CRandom.h"

/** @typedef PRandom
  * @brief Allows to use pwx::CRandom outside all namespaces.
**/
typedef ::pwx::CRandom PRandom;


#endif // PWX_PWXLIB_SRC_PRANDOM_INCLUDED
