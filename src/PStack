#pragma once
#ifndef PWX_PWXLIB_SRC_PSTACK_INCLUDED
#define PWX_PWXLIB_SRC_PSTACK_INCLUDED


/** @file PStack
  * @brief Wraps container/TStack.h and typedefs pwx::TStack to PStack.
**/
#include "container/TStack.h"

/** @typedef PStack
  * @brief Allows to use pwx::TStack outside all namespaces.
**/
template<typename data_t>
using PStack = ::pwx::TStack<data_t>;


#endif // PWX_PWXLIB_SRC_PSTACK_INCLUDED
