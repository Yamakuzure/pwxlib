#pragma once
#ifndef PWX_PWXLIB_SRC_PDOUBLELIST_INCLUDED
#define PWX_PWXLIB_SRC_PDOUBLELIST_INCLUDED


/** @file PDoubleList
  * @brief Wraps container/TDoubleList.h and typedefs pwx::TDoubleList to PDoubleList.
**/
#include "container/TDoubleList.h"

/** @typedef PDoubleList
  * @brief Allows to use pwx::TDoubleList outside all namespaces.
**/
template<typename data_t, typename elem_t = ::pwx::TDoubleElement<data_t> >
using PDoubleList = ::pwx::TDoubleList<data_t, elem_t>;


#endif // PWX_PWXLIB_SRC_PDOUBLELIST_INCLUDED
