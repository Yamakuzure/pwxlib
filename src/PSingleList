/** @file PSingleList
  * @brief Wraps container/TSingleList.h and typedefs pwx::TSingleList to PSingleList.
**/
#include "container/TSingleList.h"

/** @typedef PSingleList
  * @brief Allows to use pwx::TSingleList outside all namespaces.
**/
template<typename data_t, typename elem_t = ::pwx::TSingleElement<data_t> >
using PSingleList = ::pwx::TSingleList<data_t, elem_t>;
