#include "wavecolor/CWaveColor.h"

/** @typedef PWaveColor
  * @brief Allows to use pwx::CWaveColor outside all namespaces.
**/
typedef ::pwx::CWaveColor PWaveColor;


#endif // PWX_PWXLIB_SRC_PWAVECOLOR_INCLUDED
