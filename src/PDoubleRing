/** @file PDoubleRing
  * @brief Wraps container/TDoubleRing.h and typedefs pwx::TDoubleRing to PDoubleRing.
**/
#include "container/TDoubleRing.h"

/** @typedef PDoubleRing
  * @brief Allows to use pwx::TDoubleRing outside all namespaces.
**/
template<typename data_t, typename elem_t = ::pwx::TDoubleElement<data_t> >
using PDoubleRing = ::pwx::TDoubleRing<data_t, elem_t>;
