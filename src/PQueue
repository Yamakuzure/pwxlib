#pragma once
#ifndef PWX_PWXLIB_SRC_PQUEUE_INCLUDED
#define PWX_PWXLIB_SRC_PQUEUE_INCLUDED


/** @file PQueue
  * @brief Wraps container/TQueue.h and typedefs pwx::TQueue to PQueue.
**/
#include "container/TQueue.h"

/** @typedef PQueue
  * @brief Allows to use pwx::TQueue outside all namespaces.
**/
template<typename data_t>
using PQueue = ::pwx::TQueue<data_t>;


#endif // PWX_PWXLIB_SRC_PQUEUE_INCLUDED
