#pragma once
#ifndef PWX_PWXLIB_SRC_PLOCKGUARD_INCLUDED
#define PWX_PWXLIB_SRC_PLOCKGUARD_INCLUDED


/** @file PLockGuard
  * @brief Wraps basic/CLockGuard.h and typedefs pwx::CLockGuard to PLockGuard.
**/
#include "basic/CLockGuard.h"

/** @typedef PLockGuard
  * @brief Allows to use pwx::CLockGuard outside all namespaces.
**/
typedef ::pwx::CLockGuard PLockGuard;


#endif // PWX_PWXLIB_SRC_PLOCKGUARD_INCLUDED
